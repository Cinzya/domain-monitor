{
  "name": "domain-monitor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "missing": "0.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-scripts": "^3.3.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test *.js",
    "eject": "react-scripts eject",
    "coverage": "npm test -- --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@stryker-mutator/core": "^2.4.0",
    "@stryker-mutator/html-reporter": "^2.4.0",
    "@stryker-mutator/javascript-mutator": "^2.4.0",
    "@stryker-mutator/jest-runner": "^2.4.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/components/**",
      "!<rootDir>/node-modules/",
      "!<rootDir>/path/to/dir"

    ]
  },

  "coverageThreshold": {
    "global": {
      "statements": 100,
      "branchens": 50,
      "functions": 100,
      "lines": 100
    }
  }
}
